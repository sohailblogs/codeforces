import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int d = s.nextInt();
        int b = s.nextInt();
        for(int i = 0; i < b; i++){
            if(d % 10 == 0){
                d = d/10;
            }
            else{
                d--;
            }
        }
        System.out.println(d);
    }
}
