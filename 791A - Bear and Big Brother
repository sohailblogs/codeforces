import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int d = s.nextInt();
        int b = s.nextInt();
        int i = 0;
        for(i = 0; d <= b; i++){
            d = d * 3;
            b = b * 2;
        }
        System.out.println(i);
    }
}
