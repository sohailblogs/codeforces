import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        String n = s.next();
        char[] ch = n.toCharArray();
        Set<Character> set = new HashSet<>();
        for(char ele : ch){
            set.add(ele);
        }
        if(set.size() % 2 == 0){
            System.out.println("CHAT WITH HER!");
        }
        else{
            System.out.println("IGNORE HIM!");
        }
    
        
    }
    
}
