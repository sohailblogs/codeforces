import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        int h = s.nextInt();
        int[] d = new int[n];
        for(int i = 0; i < n; i++){
            d[i] = s.nextInt();
        }
        int count= 0;
        for(int i = 0; i < n; i++){
            if(d[i] <= h){count+=1;}
            else{count+=2;}
        }
        System.out.println(count);
        
    }
}
