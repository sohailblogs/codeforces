import java.util.Scanner;
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int[] a = new int[4];
        for(int i = 0; i < 4 ; i++){
            a[i] = s.nextInt();
        }
        int count = 0;
        Set<Integer> e = new HashSet<>();
        for(int i = 0; i < 4; i++){
            e.add(a[i]);
        }
        int c = e.size();
        System.out.println(4 - c);
    }
}
